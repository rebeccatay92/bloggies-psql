<% provide(:title, "Edit post with title: #{@current_post.title}") %>

<!-- <%= @current_post %> -->

<%= form_for @current_post do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <br>
  <%= f.label :content %>
  <%= f.text_area :content %>
  <br>
  <%= f.hidden_field :user_id %>
  <%= f.submit %>
<% end %>
